{
  "name": "app",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack-dev-server  --config webpack.develop.config.js --devtool eval --progress --colors --hot --content-base src",
    "publish": "NODE_ENV=production  webpack  --config webpack.publish.config -p   --progress --colors",
    "package": "NODE_ENV=development webpack  --config webpack.package.config -p",
    "start": "NODE_ENV=development  electron  .",
    "build-darwin": "NODE_ENV=production node_modules/.bin/electron-packager .  --darwin --overwrite  --asar=true --prune  --icon=publish/assets/logos/mac-logo.icns  --ignore=.idea --ignore=src --ignore=node_modules",
    "build-win32": "NODE_ENV=production  node_modules/.bin/electron-packager .   --win32  --overwrite  --asar=true  --prune --icon=publish/assets/logos/windows-logo.ico  --ignore=.idea --ignore=src --ignore=node_modules",
    "build-linux": "NODE_ENV=production node_modules/.bin/electron-packager .   --linux  --overwrite  --asar=true --prune --ignore=.idea --ignore=src --ignore=node_modules",
    "all": "node_modules/.bin/electron-packager . all  --asar=true   --prune   --ignore=.idea --ignore=src  --ignore=node_modules "
  },
  "author": "feel",
  "license": "ISC",
  "devDependencies": {
    "asar": "^0.12.3",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-classes": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-eslatest-node6": "^1.0.1",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-preset-stage-3": "^6.17.0",
    "css-loader": "^0.26.1",
    "devtron": "^1.4.0",
    "electron": "^1.4.13",
    "electron-builder": "^10.13.1",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.0.1",
    "electron-packager": "^8.4.0",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "react-json-tree": "^0.10.0",
    "sass-loader": "^4.1.1",
    "source-map-support": "^0.4.8",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7"
  },
  "dependencies": {
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.24.1",
    "immutable": "^3.8.1",
    "log4js": "^1.1.0",
    "material-ui": "^0.16.6",
    "nybble": "^1.0.4",
    "open-browser-webpack-plugin": "0.0.3",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^1.3.1",
    "react-router": "^3.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "traits-decorator": "^1.0.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0",
      "stage-1",
      "stage-2",
      "stage-3",
      "eslatest-node6"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  }
